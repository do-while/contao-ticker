{
    "name": "do-while/contao-ticker-bundle",
    "description": "Integration of a ticker line into a web page",
    "keywords": ["contao", "widget", "ticker"],
    "type": "contao-bundle",
    "homepage": "https://www.softleister.de",
    "license": "MIT",
    "authors": [{
        "name": "Softleister - Hagen Klemp",
        "homepage": "https://www.softleister.de",
        "role": "Project leader"
    }],
    "support": {
        "issues": "https://github.com/do-while/contao-ticker/issues",
        "source": "https://github.com/do-while/contao-ticker"
    },
    "require": {
        "php": ">=5.6.0",
        "contao/core-bundle": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Softleister\\TickerBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/cache/",
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        },
		"contao-manager-plugin": "Softleister\\TickerBundle\\ContaoManager\\Plugin"
    }
}
